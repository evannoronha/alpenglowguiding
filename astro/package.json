{
  "name": "alpenglowguiding",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build",
    "serve:cf": "wrangler dev --port=8787 ./dist/_worker.js",
    "preview": "astro preview",
    "astro": "astro",
    "deploy": "npm run build && BRANCH=$(git rev-parse --abbrev-ref HEAD) && if [ \"$BRANCH\" = \"main\" ]; then wrangler deploy; else BRANCH_CLEAN=$(echo $BRANCH | tr '/' '-' | cut -c1-25) && wrangler versions upload --tag $BRANCH_CLEAN --preview-alias $BRANCH_CLEAN; fi"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^12.0.0",
    "@astrojs/mdx": "^4.0.1",
    "@astrojs/react": "^4.0.0",
    "@astrojs/sitemap": "^3.2.1",
    "@strapi/blocks-react-renderer": "^1.0.2",
    "@types/react": "^18.3.13",
    "@types/react-dom": "^18.3.1",
    "astro": "^5.0.5",
    "astro-robots-txt": "^1.0.0",
    "astro-seo": "^0.8.4",
    "free-astro-components": "^1.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resend": "^4.0.1",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241202.0",
    "@types/nodemailer": "^6.4.17",
    "wrangler": "^4.5.1"
  }
}
