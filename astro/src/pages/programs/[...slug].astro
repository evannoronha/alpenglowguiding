---
export const prerender = false;

import { getProgramBySlug } from "../../lib/strapi";
import StrapiProgramLayout from "../../layouts/StrapiProgramLayout.astro";

const { slug } = Astro.params;

if (!slug) {
    return Astro.redirect("/404");
}

const program = await getProgramBySlug(slug);

if (!program) {
    return Astro.redirect("/404");
}
---

<StrapiProgramLayout program={program} />
